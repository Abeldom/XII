#include <iostream>
#include <fstream>
#include<vector>

using namespace std;

ifstream in("input.txt");

int szaml=0,k;


bool particio(int n,vector<int> aktualis )
{
    if(n==0)
    {
        cout<<"da";
        szaml++;
        if(k==szaml){
            for(unsigned int i=0; i<aktualis.size(); i++)
            {
                cout<<aktualis[i]<<" ";
            }
            return true;
        }
        return false;
    }

    for(int i=minim; i<=n; i++)
    {
        aktualis.push_back(i);
        if(particio(n-i,aktualis){
            return true;
        }
        aktualis.pop_back();
    }

    return false;

}


bool particio2(int n,vector<int> aktualis,vector<int> megoldasm )
{
    if(n==0)
    {
        szaml++;
        if(aktualis.size()==megoldas.size()){
            bool jo=true;

            for(unsigned int i=0; i<aktualis.size(); i++)
            {
                if(aktualis[i]!=megoldas[i])
                {
                    jo=false;
                    return false;
                }
            }
            if(jo){
                cout<<szaml;
                return true;
            }
        }else{
            return false;
        }
    }
    for(int i=minim; i<=n; i++)
    {
        aktualis.push_back(i);
        if(particio2(n-i,aktualis,megoldas)){
            return true;
        }
        aktualis.pop_back();
    }

    return false;

}

int main()
{
    int c,n;
    in>>c>>n;


    if(c==1){
        in>>k;
        vector<int>megoldas;
        particio(n,megoldas,1);
    }else{
        int x;
        vector<int>megoldas;
        while(in>>x)
        {
            megoldas.push_back(x);
        }
        vector<int>aktualis;
        particio2(n,aktualis,megoldas,1);
    }

    return 0;
}
